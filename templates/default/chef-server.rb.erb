# AUTO-GENERATED BY CHEF - DO NOT MODIFY

topology "standalone"

<% unless node['chef_server']['api_fqdn'].nil? || node['chef_server']['api_fqdn'].empty? -%>
api_fqdn "<%= node['chef_server']['api_fqdn'] %>"
<% end -%>

<% node['chef_server']['configuration'].each_pair do |component, tunables| -%>
<% case tunables -%>
<% when Hash -%>
<% tunables.each_pair do |name, value| -%>
<% value = %Q["#{value}"] if value.kind_of?(String) -%>
<%= "#{component.gsub("-","_")}['#{name}'] = #{value}" %>
<% end -%>
<% when String -%>
<%= component.gsub('-', '_') %> "<%= tunables %>"
<% end -%>
<% end -%>